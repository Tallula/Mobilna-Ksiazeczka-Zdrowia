<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WeterynarzOkno">

    <LinearLayout
        android:layout_width="349dp"
        android:layout_height="476dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="50dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.161">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:text="Zeskanuj QR badanego psa" />

        <Button
            android:id="@+id/odczytajQRPsaButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="odczytaj QR badanego psa" />

        <TextView
            android:id="@+id/testTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Zaplanuj wizyte" />

        <Button
            android:id="@+id/zapiszWizyteButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Zaplanuj wizyte" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <Button
            android:id="@+id/zeskanujRFIDButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Zeskanuj RFID" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>